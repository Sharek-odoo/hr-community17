<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data noupdate="1">
        <record id="hr_salary_rule_reward_allowance" model="hr.salary.rule">
            <field name="amount_select">code</field>
             <!-- <field name="struct_id" ref="sharek_hr_payroll_extension.ksa_basic_structure"/> -->
             <field name="condition_select">none</field>
            <field name="amount_python_compute">result = payslip.reward_allowance</field>
            <field name="code">REWARD</field>
            <field name="category_id" ref="om_hr_payroll.ALW"/>
            <field name="name">Reward Allowance</field>
            <field name="sequence" eval="197"/>
        </record>
