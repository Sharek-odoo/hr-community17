        <field name="prefix">CLAIM-</field>
        <field name="padding">4</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
    </record>

    <record id="seq_perpetual_custody" model="ir.sequence">
        <field name="name">Perpetual Custody</field>
