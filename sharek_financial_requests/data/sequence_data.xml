        <field name="prefix">TEMPCUS-</field>
        <field name="padding">4</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
    </record>

    <record id="seq_overtime_request" model="ir.sequence">
        <field name="name">Overtime Request</field>
        <field name="code">overtime.request</field>
        <field name="prefix">OVER-</field>
        <field name="padding">4</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
    </record>
</odoo>
