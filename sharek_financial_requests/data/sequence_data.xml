<odoo>
    <record id="seq_children_education_allowance" model="ir.sequence">
        <field name="name">Children Education Allowance</field>
        <field name="code">children.education.allowance</field>
        <field name="prefix">EDU-</field>
        <field name="padding">4</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
    </record>

    <record id="seq_salary_advance" model="ir.sequence">
        <field name="name">Salary Advance</field>
        <field name="code">salary.advance.allowance</field>
        <field name="prefix">SAD-</field>
        <field name="padding">4</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
    </record>

     <record id="seq_financial_claim" model="ir.sequence">
        <field name="name">Financial Claim</field>
