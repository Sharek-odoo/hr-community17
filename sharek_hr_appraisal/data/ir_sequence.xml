<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Sequences for appraisal.assignment -->
        <record id="seq_appraisal_assigment" model="ir.sequence">
            <field name="name">Appraisal Assignment</field>
            <field name="code">appraisal.assignment</field>
            <field name="prefix">APCASS</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for appraisal.employee -->
        <record id="seq__emp_appraisal" model="ir.sequence">
            <field name="name">Employee Appraisal</field>
            <field name="code">employee.appraisal</field>
            <field name="prefix">APP</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>




    </data>
</odoo>