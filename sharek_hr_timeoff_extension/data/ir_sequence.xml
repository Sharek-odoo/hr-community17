<?xml version="1.0" encoding="UTF-8"?>
<odoo noupdate="1">

	<record id="seq_hr_leave" model="ir.sequence" forcecreate="False">
		<field name="name">Leave Request</field>
		<field name="code">hr.leave</field>
		<field name="padding">4</field>
		<field name="prefix">LR</field>
		<field name="company_id" eval="False" />
	</record>

	<record id="seq_timeoff_transfer" model="ir.sequence">
        <field name="name">Time Off Transfer</field>
        <field name="code">timeoff.transfer</field>
        <field name="prefix">TFT</field>
        <field name="padding">4</field>
    </record>
		
</odoo>